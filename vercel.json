{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/admin.html",
      "dest": "/admin.html"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/config.js",
      "dest": "/config.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
